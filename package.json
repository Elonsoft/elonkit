{
  "name": "elonsoft",
  "version": "0.0.1",
  "scripts": {
    "start": "yarn workspace @elonsoft/elonkit start",
    "build": "ng build --prod && scss-bundle -c scss-bundle.config.json && cp {LICENSE,CHANGELOG.md,README.md} dist/elonkit ",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "lint:tslint": "tslint -c tslint.json --project tsconfig.json 'projects/**/*.ts'",
    "lint:stylelint": "stylelint projects"
  },
  "private": true,
  "workspaces": [
    "projects/elonkit/src"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "tslint -c ./tslint.json --project tsconfig.json",
    "*.{css,scss}": "stylelint"
  },
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/cdk": "~9.2.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/material": "~9.2.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "angular2-text-mask": "^9.0.0",
    "rxjs": "~6.5.5",
    "text-mask-addons": "^3.8.0",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^9.0.1",
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@babel/core": "~7.9.0",
    "@compodoc/compodoc": "~1.1.11",
    "@material-ui/core": "^4.9.9",
    "@storybook/addon-a11y": "~5.3.18",
    "@storybook/addon-actions": "~5.3.18",
    "@storybook/addon-docs": "~5.3.18",
    "@storybook/addon-knobs": "~5.3.18",
    "@storybook/addon-viewport": "~5.3.18",
    "@storybook/angular": "~5.3.18",
    "@testing-library/angular": "~9.1.0",
    "@testing-library/jest-dom": "~5.3.0",
    "@types/jest": "~25.2.1",
    "@types/node": "^13.11.0",
    "babel-loader": "~8.1.0",
    "codelyzer": "^5.2.2",
    "css-loader": "~3.4.2",
    "husky": "~4.2.3",
    "jest": "~25.2.7",
    "lint-staged": "~10.1.2",
    "ng-packagr": "^9.1.0",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.3",
    "react": "~16.13.1",
    "react-is": "~16.13.1",
    "sass-loader": "^8.0.2",
    "scss-bundle": "^3.1.1",
    "stylelint": "~13.3.0",
    "stylelint-config-sass-guidelines": "~7.0.0",
    "stylelint-order": "~4.0.0",
    "ts-node": "~8.8.2",
    "tslint": "~6.1.1",
    "tslint-config-prettier": "~1.18.0",
    "tslint-plugin-prettier": "~2.3.0",
    "typescript": "~3.8.3"
  }
}
